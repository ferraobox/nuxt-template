{
  "version": 2,
  "alias": ["ferraobox-nuxt-template.vercel.app"],
  "github": {
    "autoAlias": false,
    "enabled": false
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    },
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": ["data/**", "modules/**", "utils/**"]
      }
    }
  ]
}
