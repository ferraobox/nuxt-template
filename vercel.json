{
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": [
          "data/**",
          "modules/**",
          "utils/**",
          ".nuxt/**",
          "node_modules/**",
          "nuxt.config.js",
          "package.json"
        ]
      }
    }
  ],
  "alias": ["ferraobox-nuxt-template.vercel.app"],
  "github": {
    "autoAlias": false,
    "enabled": false
  }
}
